{
	"env": {
		"browser": true,
		"es6": true,
		"node": true
	},
	"extends": [
		"standard"
	],
	"globals": {
		"Atomics": "readonly",
		"SharedArrayBuffer": "readonly"
	},
	"parserOptions": {
		"ecmaFeatures": {
		"jsx": true
	},
	"ecmaVersion": 2018,
	"sourceType": "module"
	},
	"plugins": [
		"react"
	],
	"rules": {
		"quote-props": "off",
		"quotes": "off",
		"semi": "off",
		"standard/no-callback-literal": "off",
		"no-unused-vars": "off",
		"no-unused-expressions": "off",
		"eqeqeq": "off"
	}
}